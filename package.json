{
  "name": "novelbox",
  "version": "1.1.0",
  "description": "AI辅助小说创作工具 - AI-assisted novel writing tool",
  "main": "electron/main.js",
  "author": {
    "name": "NovelBox Team",
    "email": "contact@novelbox.app"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/MisonL/novelbox",
  "repository": {
    "type": "git",
    "url": "https://github.com/MisonL/novelbox.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "vite build --config vite.config.electron.ts && electron .",
    "electron:build": "vite build --config vite.config.electron.ts && electron-builder",
    "electron:release": "vite build --config vite.config.electron.ts && electron-builder --publish=never",
    "electron:build:win": "vite build --config vite.config.electron.ts && npx @electron/packager . NovelBox --platform=win32 --arch=x64 --out=release --overwrite --asar=false --ignore=node_modules/.pnpm-debug.log --ignore=dist-web --ignore=scripts --ignore=config --ignore=docker --ignore=docs --ignore=venv --ignore=.git --prune=false",
    "electron:build:mac": "vite build --config vite.config.electron.ts && npx @electron/packager . NovelBox --platform=darwin --arch=x64 --out=release --overwrite --asar=false --ignore=node_modules/.pnpm-debug.log --ignore=dist-web --ignore=scripts --ignore=config --ignore=docker --ignore=docs --ignore=venv --ignore=.git --prune=false",
    "electron:build:linux": "vite build --config vite.config.electron.ts && npx @electron/packager . NovelBox --platform=linux --arch=x64 --out=release --overwrite --asar=false --ignore=node_modules/.pnpm-debug.log --ignore=dist-web --ignore=scripts --ignore=config --ignore=docker --ignore=docs --ignore=venv --ignore=.git --prune=false",
    "web:dev": "vite --config config/vite.config.web.ts",
    "web:build": "vite build --config config/vite.config.web.ts",
    "web:preview": "vite preview --config config/vite.config.web.ts",
    "web:docker:dev": "docker-compose -f docker/docker-compose.yml up --build",
    "web:docker:prod": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.prod.yml up --build",
    "clean": "node scripts/clean.cjs",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@google/generative-ai": "^0.17.1",
    "@types/node-fetch": "^2.6.13",
    "@types/web-bluetooth": "^0.0.21",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^11.0.3",
    "@vueuse/metadata": "14.0.0-alpha.0",
    "@vueuse/shared": "14.0.0-alpha.0",
    "abort-controller": "^3.0.0",
    "agentkeepalive": "^4.6.0",
    "asynckit": "^0.4.0",
    "axios": "^1.7.7",
    "buffer": "^6.0.3",
    "call-bind-apply-helpers": "^1.0.2",
    "combined-stream": "^1.0.8",
    "data-uri-to-buffer": "^6.0.2",
    "delayed-stream": "^1.0.0",
    "diff-match-patch": "^1.0.5",
    "docx": "^8.5.0",
    "dunder-proto": "^1.0.1",
    "element-plus": "^2.8.1",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es-object-atoms": "^1.1.1",
    "es-set-tostringtag": "^2.1.0",
    "event-target-shim": "^6.0.2",
    "eventemitter3": "^5.0.1",
    "fast-diff": "^1.3.0",
    "fetch-blob": "^4.0.0",
    "follow-redirects": "^1.15.11",
    "form-data": "^4.0.4",
    "form-data-encoder": "^4.1.0",
    "formdata-node": "^6.0.3",
    "formdata-polyfill": "^4.0.10",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.3.0",
    "get-proto": "^1.0.1",
    "gopd": "^1.2.0",
    "has-symbols": "^1.1.0",
    "has-tostringtag": "^1.0.2",
    "hasown": "^2.0.2",
    "html-to-text": "^9.0.5",
    "humanize-ms": "^2.0.0",
    "lodash-es": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "math-intrinsics": "^1.1.0",
    "mime-db": "^1.54.0",
    "mime-types": "^3.0.1",
    "ms": "^2.1.3",
    "node-domexception": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.57.0",
    "parchment": "^3.0.0",
    "process": "^0.11.10",
    "proxy-from-env": "^1.1.0",
    "quill": "^2.0.2",
    "quill-delta": "^5.1.0",
    "uuid": "^10.0.0",
    "vue": "^3.4.38",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "@types/diff-match-patch": "^1.0.36",
    "@types/node": "^22.5.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "base64-js": "^1.5.1",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "events": "^3.3.0",
    "ieee754": "^1.2.1",
    "immediate": "^3.3.0",
    "jszip": "^3.10.1",
    "lie": "^3.3.0",
    "pako": "^2.1.0",
    "postcss": "^8.4.41",
    "readable-stream": "^4.7.0",
    "string_decoder": "^1.3.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "undici-types": "^7.15.0",
    "vite": "^5.4.2",
    "vue-tsc": "^2.0.29"
  },
  "build": {
    "appId": "com.novelbox.app",
    "productName": "NovelBox",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "asar": false,
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon-512.png"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon-512.png"
    },
    "linux": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon-512.png",
      "category": "Office"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}