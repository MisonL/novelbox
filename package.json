{
  "name": "novelbox",
  "version": "1.2.0",
  "description": "AI辅助小说创作工具 - AI-assisted novel writing tool",
  "type": "module",
  "main": "electron/main.cjs",
  "author": {
    "name": "NovelBox Team",
    "email": "contact@novelbox.app"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/MisonL/novelbox",
  "repository": {
    "type": "git",
    "url": "https://github.com/MisonL/novelbox.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "vite build --config vite.config.electron.mts && electron .",
    "electron:build": "vite build --config vite.config.electron.mts && electron-builder",
    "electron:release": "vite build --config vite.config.electron.mts && electron-builder --publish=never",
    "electron:build:win": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=win32 --arch=x64 --out=release --overwrite --asar --prune=false",
    "electron:build:win-arm64": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=win32 --arch=arm64 --out=release --overwrite --asar --prune=false",
    "electron:build:mac": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=darwin --arch=x64 --out=release --overwrite --asar --prune=false",
    "electron:build:mac-arm64": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=darwin --arch=arm64 --out=release --overwrite --asar --prune=false",
    "electron:build:linux": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=linux --arch=x64 --out=release --overwrite --asar --prune=false",
    "electron:build:linux-arm64": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=linux --arch=arm64 --out=release --overwrite --asar --prune=false",
    "electron:build:linux-arm": "vite build --config vite.config.electron.mts && npx @electron/packager . NovelBox --platform=linux --arch=armv7l --out=release --overwrite --asar --prune=false",
    "web:dev": "vite --config config/vite.config.web.ts",
    "web:build": "vite build --config config/vite.config.web.ts",
    "web:preview": "vite preview --config config/vite.config.web.ts",
    "web:docker:dev": "docker-compose -f docker/docker-compose.yml up --build",
    "web:docker:prod": "docker-compose -f docker/docker-compose.yml -f docker/docker-compose.prod.yml up --build",
    "clean": "node scripts/clean.cjs",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "test": "vitest run",
    "spec:check": "node scripts/spec-kit.cjs check",
    "spec:validate": "node scripts/spec-kit.cjs validate",
    "spec:test": "node scripts/spec-kit.cjs test",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "@element-plus/icons-vue": "^2.3.2",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^13.9.0",
    "@vueuse/metadata": "14.0.0-alpha.0",
    "@vueuse/shared": "13.9.0",
    "abort-controller": "^3.0.0",
    "agentkeepalive": "^4.6.0",
    "asynckit": "^0.4.0",
    "braces": "^3.0.3",
    "buffer": "^6.0.3",
    "call-bind-apply-helpers": "^1.0.2",
    "combined-stream": "^1.0.8",
    "concat-stream": "^2.0.0",
    "data-uri-to-buffer": "^6.0.2",
    "delayed-stream": "^1.0.0",
    "diff-match-patch": "^1.0.5",
    "docx": "^9.5.1",
    "dunder-proto": "^1.0.1",
    "element-plus": "2.9.8",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es-object-atoms": "^1.1.1",
    "es-set-tostringtag": "^2.1.0",
    "event-target-shim": "^6.0.2",
    "eventemitter3": "^5.0.1",
    "fast-diff": "^1.3.0",
    "fetch-blob": "^4.0.0",
    "follow-redirects": "^1.15.11",
    "form-data": "^4.0.4",
    "form-data-encoder": "^4.1.0",
    "formdata-node": "^6.0.3",
    "formdata-polyfill": "^4.0.10",
    "function-bind": "^1.1.2",
    "get-intrinsic": "^1.3.0",
    "get-proto": "^1.0.1",
    "gopd": "^1.2.0",
    "has-symbols": "^1.1.0",
    "has-tostringtag": "^1.0.2",
    "hasown": "^2.0.2",
    "html-to-text": "^9.0.5",
    "humanize-ms": "^2.0.0",
    "math-intrinsics": "^1.1.0",
    "micromatch": "^4.0.8",
    "mime-db": "^1.54.0",
    "mime-types": "^3.0.1",
    "ms": "^2.1.3",
    "mysql2": "^3.15.3",
    "node-fetch": "^3.3.2",
    "parchment": "^3.0.0",
    "process": "^0.11.10",
    "proxy-from-env": "^1.1.0",
    "quill": "^2.0.3",
    "quill-delta": "^5.1.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "vue": "^3.5.13",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.5.1",
    "yargs-parser": "^22.0.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "@electron/rebuild": "^4.0.1",
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/diff-match-patch": "^1.0.36",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.0",
    "asar": "^3.2.0",
    "autoprefixer": "^10.4.20",
    "crypto-browserify": "^3.12.1",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.10",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^10.5.0",
    "events": "^3.3.0",
    "jsdom": "^27.0.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.45",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "^5.6.2",
    "util": "^0.12.5",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.8"
  },
  "build": {
    "appId": "com.novelbox.app",
    "productName": "NovelBox",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json",
      "!**/node_modules/**",
      "!node_modules/.pnpm-debug.log",
      "!dist-web",
      "!scripts",
      "!config",
      "!docker",
      "!docs",
      "!venv",
      "!.git",
      "node_modules/@electron/remote/**/*"
    ],
    "asar": true,
    "asarUnpack": [],
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon-512.png"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon-512.png",
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "asar": true
    },
    "linux": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/icon-512.png",
      "category": "Office"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "pnpm": {
    "overrides": {},
    "ignoredBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "fs-xattr",
      "macos-alias"
    ]
  },
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40"
}
